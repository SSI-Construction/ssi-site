<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in collections.all %}
    {% if page.url %}
      <url>
        <loc>{{ metadata.url }}{{ page.url | url }}</loc>
        <priority>{% if page.url == "/" %}1.0{% else %}0.5{% endif %}</priority>
      </url>
    {% endif %}
  {% endfor %}
</urlset>
